                                    <input class="form-number" type="number" id="criterion-n_estimators-input" name="nthread" style="width: 230px;" placeholder="Enter a positive integer value" min="1" max="100" step="1" value="5" required oninput="this.value = this.value.replace(/[^\d.]/g,'')">
