                                    <option value="reg:squarederror ">reg:squarederror </option>
                                    <option value="reg:logistic ">reg:logistic </option>
                                    <option value="reg:gamma">reg:gamma</option>
                                    <option value="rank:pairwise">rank:pairwise</option>
                                    <option value="rank:ndcg">rank:ndcg</option>
                                    <option value="rank:map">rank:map</option>
                                    <option value="survival:cox ">survival:cox </option>
                                </select>
                                <div class="explication">
                                    <p>
